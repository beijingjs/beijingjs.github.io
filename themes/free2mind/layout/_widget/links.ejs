<% if (theme.links.length){ %>
<div class="widget">
	<h4 style="font-weight: bold; color: #333;font-family: "Helvetica Neue", Helvetica, Arial, sans-serif"><%= __('links') %></h4>
	<ul class="blogroll list-unstyled">
	<% for (var i in theme.links){ %>
		<li style="font-size:14px"><i class="<%- theme.links[i].icon %>"></i><a href="<%- theme.links[i].url %>" title="<%- theme.links[i].intro %>" target="_blank"]);"><%= theme.links[i].title %></a></li>
	<% } %>
	</ul>
</div>
<% } %>
